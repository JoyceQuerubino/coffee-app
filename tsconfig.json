{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@assets/*": [
        "./src/assets/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@screens/*": [
        "./src/screens/*"
      ],
      "@contexts/*": [
        "./src/contexts/*"
      ],
      "@services/*": [
        "./src/services/*"
      ],
      "@types/*": [
        "./src/types/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@helpers/*": [
        "./src/helpers/*"
      ],
      "@theme/*": [
        "./src/theme/*"
      ],
      "@tests/*": [
        "src/tests/*"
      ]
    }
  }
}
